{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "16"
    }
  },
  "ignorePatterns": ["**/_this_is_virtual_fs_path_/**/*"],
  "rules": {
    "strict": 0,
    "semi": ["error", "always"],
    "indent": ["error", 2, { "SwitchCase": 1, "MemberExpression": "off" }],
    "no-console": "off",
    "no-debugger": "off",
    "react/prop-types": "off"
  }
}
